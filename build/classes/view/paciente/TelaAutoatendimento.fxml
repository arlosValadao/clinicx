<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="422.0" prefWidth="621.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.paciente.TelaAutoatendimentoController">
   <children>
      <Button fx:id="btMinhasConsultas" layoutX="430.0" layoutY="320.0" mnemonicParsing="false" onAction="#minhasConsultas" text="Minhas Consultas">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <DatePicker fx:id="datePicker" layoutX="68.0" layoutY="96.0" onAction="#carregarHorarioDisponivel" promptText="Escolha uma data" />
      <Label layoutX="86.0" layoutY="43.0" text="Dias Disponíveis">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="333.0" layoutY="43.0" text="Horários Disponíveis">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboBoxHora" layoutX="354.0" layoutY="96.0" onAction="#carregarMinutos" prefHeight="25.0" prefWidth="60.0" />
      <ComboBox fx:id="comboBoxMin" layoutX="419.0" layoutY="96.0" prefHeight="25.0" prefWidth="60.0" />
      <Button fx:id="btMarcarConsulta" layoutX="76.0" layoutY="320.0" mnemonicParsing="false" onAction="#marcarConsulta" text="Marcar Consulta">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="btCancelar" layoutX="279.0" layoutY="320.0" mnemonicParsing="false" onAction="#cancelar" text="Cancelar">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <ComboBox fx:id="comboBoxMedico" layoutX="68.0" layoutY="200.0" onAction="#carregarComboBoxMedicos" prefWidth="150.0" />
      <ComboBox fx:id="comboBoxEspecialidade" layoutX="242.0" layoutY="200.0" onAction="#carregarComboBoxEspecialidades" prefWidth="150.0" />
      <Label layoutX="115.0" layoutY="160.0" text="Medico">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="280.0" layoutY="160.0" text="Especialidade">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
