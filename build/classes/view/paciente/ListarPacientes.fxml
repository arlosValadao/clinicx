<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="485.0" prefWidth="636.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.paciente.ListarPacientesController">
   <children>
      <TableView fx:id="tableViewPacientes" layoutX="34.0" layoutY="45.0" prefHeight="396.0" prefWidth="431.0">
        <columns>
          <TableColumn fx:id="colunaNome" prefWidth="75.0" text="Nome" />
          <TableColumn fx:id="colunaCPF" prefWidth="75.0" text="CPF" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btRemover" layoutX="486.0" layoutY="122.0" mnemonicParsing="false" onAction="#removerPaciente" prefHeight="39.0" prefWidth="123.0" text="Remover">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btEditar" layoutX="486.0" layoutY="67.0" mnemonicParsing="false" onAction="#editarPaciente" prefHeight="39.0" prefWidth="123.0" text="Editar">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btHConsultas" layoutX="486.0" layoutY="175.0" mnemonicParsing="false" onAction="#mostrarHistoricoConsultaPaciente" text="H. Consultas">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
