<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="532.0" prefWidth="663.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.medico.TelaListarMedicosController">
   <children>
      <TableView fx:id="tableViewMedicos" layoutX="34.0" layoutY="45.0" prefHeight="396.0" prefWidth="466.0">
        <columns>
          <TableColumn fx:id="colunaNome" prefWidth="75.0" text="Nome" />
          <TableColumn fx:id="colunaCPF" prefWidth="75.0" text="CPF" />
            <TableColumn fx:id="colunaEspecialidade" prefWidth="75.0" text="Especialidade" />
            <TableColumn fx:id="colunaSubespecialidade" prefWidth="108.0" text="Subespecialidade" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btRemover" layoutX="532.0" layoutY="109.0" mnemonicParsing="false" onAction="#removerMedico" prefHeight="39.0" prefWidth="101.0" text="Remover">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btEditar" layoutX="532.0" layoutY="45.0" mnemonicParsing="false" onAction="#editarMedico" prefHeight="39.0" prefWidth="101.0" text="Editar">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btConsultas" layoutX="532.0" layoutY="172.0" mnemonicParsing="false" onAction="#listarConsultasMedico" text="Consultas">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
