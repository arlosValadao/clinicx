      <Button fx:id="btRemover" layoutX="392.0" layoutY="83.0" mnemonicParsing="false" onAction="#removerEspecialidade" prefHeight="39.0" prefWidth="138.0" text="Remover">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btEditar" layoutX="392.0" layoutY="23.0" mnemonicParsing="false" onAction="#editarEspecialidade" prefHeight="39.0" prefWidth="138.0" text="Editar">
